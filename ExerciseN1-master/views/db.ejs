<!DOCTYPE html>
<html lang="en">


<body class="text-center">
<div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">

    <h1 class="cover-heading">Anime List</h1>
        <table id="Movies List">
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Rating</th>
            </tr>

        <tbody>
<% moviesList.forEach((m) => { %>
<tr>
    <td><%= m.id %></td>
        <td> <%= m.name %> </td>
        <td> <%= m.rating %> </td>
</tr>
<% }); %>
        </tbody>
        </table>

    </div>


</body>
</html>


